{
  "name": "native-node-electron-example",
  "version": "0.0.1",
  "description": "Demonstrates how to add a native node modules to an Electron application and ensure the module is bundled with the app using relative paths.",
  "main": "main.js",
  "repository": "https://github.com/jvandernoot/native-node-electron-example.git",
  "author": "Jamie VanDernoot <jamesvandernoot@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "devDependencies": {
    "electron": "^6.0.12",
    "electron-builder": "^21.2.0",
    "electron-webpack": "^2.7.4",
    "webpack": "^4.41.2"
  },
  "dependencies": {
    "keytar": "^5.0.0-beta.3"
  },
  "electronWebpack": {
      "title": true,
      "main": {
          "sourceDirectory": "./"
      },
      "renderer": {
        "sourceDirectory": "./",
        "template": "index.html"
      }
  }
}
